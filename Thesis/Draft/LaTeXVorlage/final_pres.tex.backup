 \frametitle{Membrane Vibrations}
\begin{exampleblock}{Membrane EOM}

\begin{equation}\label{membraneequation1}
 -\partial^2_tu(r,\phi;t)-2\alpha\partial_t u(r,\phi;t)+c^2_M\nabla^2u(r,\phi;t)
 =\frac{1}{\rho_m d}\Psi(r,\phi;t)
\end{equation}

 \end{exampleblock}

